NET sysclk_p LOC = J9 | DIFF_TERM = TRUE | TNM_NET = sysclk;
NET sysclk_n LOC = H9 | DIFF_TERM = TRUE;

TIMESPEC TS_sysclk = PERIOD sysclk 200MHz;

TIMESPEC TS_cdc_tig_a = FROM clk125 TO ipb_clk TIG;
TIMESPEC TS_cdc_tig_b = FROM ipb_clk TO clk125 TIG;
NET "clocks/rst*" TIG;

NET leds<0> LOC=AC22;
NET leds<1> LOC=AC24;
NET leds<2> LOC=AE22;
NET leds<3> LOC=AE23;
NET leds<4> LOC=AB23;
NET leds<5> LOC=AG23;
NET leds<6> LOC=AE24;
NET leds<7> LOC=AD24;

# Ethernet PHY
TIMEGRP TG_gmii_tx=PADS("gmii_tx*");
TIMEGRP TG_gmii_tx OFFSET = OUT AFTER sysclk_p REFERENCE_PIN "gmii_gtx_clk" RISING;

NET gmii_gtx_clk LOC=AH12 | SLEW=FAST;
NET gmii_txd<0> LOC=AM11;
NET gmii_txd<1> LOC=AL11;
NET gmii_txd<2> LOC=AG10;
NET gmii_txd<3> LOC=AG11;
NET gmii_txd<4> LOC=AL10;
NET gmii_txd<5> LOC=AM10;
NET gmii_txd<6> LOC=AE11;
NET gmii_txd<7> LOC=AF11;
NET gmii_tx_en LOC=AJ10;
NET gmii_tx_er LOC=AH10;

NET gmii_rx_clk LOC=AP11 | TNM_NET= "gmii_rx_clk";
TIMESPEC "TS_GMII_RX_CLK" = PERIOD "gmii_rx_clk" 125MHz;
OFFSET = IN 2.5ns VALID 3ns BEFORE gmii_rx_clk;
NET gmii_rxd<0> LOC=AN13;
NET gmii_rxd<1> LOC=AF14;
NET gmii_rxd<2> LOC=AE14;
NET gmii_rxd<3> LOC=AN12;
NET gmii_rxd<4> LOC=AM12;
NET gmii_rxd<5> LOC=AD11;
NET gmii_rxd<6> LOC=AC12;
NET gmii_rxd<7> LOC=AC13;
NET gmii_rx_dv LOC=AM13;
NET gmii_rx_er LOC=AG12;

NET phy_rstb LOC=AH13;

NET userio_p LOC=V34;
NET userio_n LOC=W34;
